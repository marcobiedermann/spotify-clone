{
  "name": "spotify-clone",
  "version": "1.0.0",
  "description": "Simple Spotify clone using their Web API.",
  "main": "client/index.html",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.production.config.ts",
    "check": "tsc --noEmit",
    "develop": "webpack serve --config webpack.development.config.ts",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:css": "tcm -p 'client/**/*.module.css' .",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "server": "json-server db.json",
    "test": "jest --color --runInBand",
    "test:ci": "npm run test -- --ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/spotify-clone.git"
  },
  "keywords": [
    "api",
    "clone",
    "front-end",
    "music",
    "spotify",
    "ui"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/spotify-clone/issues"
  },
  "homepage": "https://github.com/marcobiedermann/spotify-clone#readme",
  "devDependencies": {
    "@types/classnames": "^2.3.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.8",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.3.5",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "swr": "^0.5.6"
  }
}
