{
  "name": "spotify-clone",
  "version": "1.0.0",
  "description": "Simple Spotify clone using their Web API.",
  "main": "client/index.html",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.production.config.ts",
    "check": "tsc --noEmit",
    "develop": "webpack serve --config webpack.development.config.ts",
    "format": "prettier --write .",
    "generate:css": "tcm -p 'client/**/*.module.css' .",
    "lint:css": "stylelint '**/*.css'",
    "lint:js": "eslint '**/*.{ts,tsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "server": "json-server db.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/spotify-clone.git"
  },
  "keywords": [
    "api",
    "clone",
    "front-end",
    "music",
    "spotify",
    "ui"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/spotify-clone/issues"
  },
  "homepage": "https://github.com/marcobiedermann/spotify-clone#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "ts-node": "^9.1.1",
    "typed-css-modules": "^0.6.4",
